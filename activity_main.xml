<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="409dp"
        android:layout_height="98dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Welcome to Nethan's store!"
        app:titleTextColor="@color/white"
        tools:layout_editor_absoluteX="2dp" />

    <ImageButton
        android:id="@+id/produce"
        android:layout_width="277dp"
        android:layout_height="171dp"
        android:src="@drawable/produce"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.763" />

    <TextView
        android:id="@+id/produce_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Produce"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="48sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/produce"
        app:layout_constraintEnd_toEndOf="@+id/produce"
        app:layout_constraintStart_toStartOf="@+id/produce"
        app:layout_constraintTop_toTopOf="@+id/produce"
        app:layout_constraintVertical_bias="0.448" />

    <ImageButton
        android:id="@+id/meat"
        android:layout_width="277dp"
        android:layout_height="171dp"
        android:backgroundTint="@color/red"
        android:src="@drawable/meat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.346" />

    <TextView
        android:id="@+id/meat_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meat"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="48sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/meat"
        app:layout_constraintEnd_toEndOf="@+id/meat"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/meat"
        app:layout_constraintTop_toTopOf="@+id/meat"
        app:layout_constraintVertical_bias="0.495" />

</androidx.constraintlayout.widget.ConstraintLayout>
